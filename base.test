  <script>
	$("#popup .close").click(function(){
		$("#popup").fadeOut();
		return false;
	})

	$("div.support a").click(function(){
		if($("#popup:visible").length)
		{
			$("#popup").fadeOut();
		}
		else
		{
			$("#popup").fadeIn();
		}
		return false;
	})



$("div.projects a.close").click(function(){
		$("div.projects").animate({ width:'0px', opacity:'0'}, 250, function(){$("div.projects").hide()})
		return false;
	})

	$("div.projects a.my_lan").click(function(){
		$("div.projects").animate({ width:'0px', opacity:'0'}, 250, function(){$("div.projects").hide()})
		return false;
	})

	$("div.projects").click(function(){
		$("div.projects").animate({ width:'0px', opacity:'0'}, 250, function(){$("div.projects").hide()})
		return false;
	})


	$("a.openProjects.link").click(function(){
		
		$("div.projects").show().animate({ width:'75%', opacity:'1'}, 250)
		return false;
	})

	$("div.oldSite div.close *").click(function(){
		$("div.oldSite").animate({height:'0px'}, 500, function(){
			$(this).hide()
		})
	})

	$("a.sitemap").click(function(){	
		$("div.sitemap").toggle()
			return false;
	})

	$("button.button2").click(function(){
		$(this).parent().parent().animate({opacity: '0'}, 500, function() {

	$(this).find('*').hide()

		$(this).css("height","0px")
		$(this).animate({ width: 'toggle'}, 500, 'swing', function(){
			if($(this).parent().find("*:visible").length == 0)
			{
				$(this).parent().hide()
			}
		})
	  });

	})
</script>


